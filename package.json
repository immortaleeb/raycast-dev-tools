{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dev-tools",
  "title": "Dev Tools",
  "description": "This makes various dev tools available as Raycast commands",
  "icon": "dev-tools.png",
  "author": "enverb",
  "license": "MIT",
  "commands": [
    {
      "name": "generate-uuid",
      "title": "Generate Uuid",
      "description": "Generates a v4 uuid",
      "mode": "view"
    },
    {
      "name": "generate-uuids",
      "title": "Generate Multiple Uuids",
      "description": "Generates multiple v4 uuids",
      "mode": "view"
    },
    {
      "name": "base64-encode",
      "title": "Base64 Encode",
      "description": "Base64 encodes your clipboard",
      "mode": "view"
    },
    {
      "name": "base64-decode",
      "title": "Base64 Decode",
      "description": "Base64 decodes your clipboard",
      "mode": "view"
    },
    {
      "name": "url-encode",
      "title": "URL Encode",
      "description": "URL encode your clipboard",
      "mode": "view"
    },
    {
      "name": "url-decode",
      "title": "URL Decode",
      "description": "URL decodes your clipboard",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
